{
  "name": "mindustry-chatbot",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:cloudflare": "turbo run build:cloudflare --filter=web",
    "deploy:cloudflare": "turbo run deploy:cloudflare --filter=web",
    "versions:upload": "turbo run versions:upload --filter=web",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "upload": "turbo run upload",
    "preview": "turbo run preview",
    "cf-typegen": "turbo run cf-typegen",
    "analyze": "turbo run analyze",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "lefthook": "^2.1.1",
    "turbo": "^2.8.12",
    "ultracite": "7.2.4"
  }
}
